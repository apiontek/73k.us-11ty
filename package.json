{
  "name": "73k.us-11ty",
  "version": "1.0.0",
  "description": "73k.us, personal website of Adam Piontek, made with 11ty",
  "scripts": {
    "clean": "npx del-cli -f _site",
    "serve": "cross-env ELEVENTY_ENV=dev npx @11ty/eleventy --serve --quiet",
    "serve:prod": "cross-env ELEVENTY_ENV=prod npx @11ty/eleventy --serve --quiet",
    "start": "npm run serve",
    "start:prod": "npm run serve:prod",
    "dev": "npm run serve",
    "dev:prod": "npm run serve:prod",
    "build": "cross-env ELEVENTY_ENV=dev npx @11ty/eleventy",
    "prebuild:prod": "npm run clean",
    "build:prod": "cross-env ELEVENTY_ENV=prod npx @11ty/eleventy",
    "format:jsprettier": "npx prettier eleventy*.js ./{content,_data,_includes}/**/*.js --write",
    "format:jseslint": "npx eslint eleventy*.js ./{content,_data,_includes}/**/*.js --ext .js --fix",
    "format:js": "npm run format:jsprettier & npm run format:jseslint",
    "format:json": "npx prettier package.json .eslintrc.json --write",
    "format": "npm run format:json & npm run format:js",
    "lint": "npx eslint eleventy*.js ./{content,_data,_includes}/**/*.js --ext .js",
    "debug": "DEBUG=Eleventy* npx @11ty/eleventy",
    "debugstart": "DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
    "benchmark": "DEBUG=Eleventy:Benchmark* npx @11ty/eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:apiontek/73k.us-11ty.git"
  },
  "author": {
    "name": "Adam Piontek",
    "email": "adam@73k.us",
    "url": "https://73k.us/"
  },
  "license": "CC BY-NC-SA 4.0",
  "engines": {
    "node": ">=20"
  },
  "bugs": {
    "url": "https://github.com/apiontek/73k.us-11ty/issues"
  },
  "homepage": "https://github.com/apiontek/73k.us-11ty#readme",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^3.1.0",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-bundle": "^1.0.4",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "browserslist": "^4.21.9",
    "child_process": "^1.0.2",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "esbuild": "^0.18.6",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "lightningcss": "^1.21.1",
    "luxon": "^3.3.0",
    "markdown-it-anchor": "^8.6.7",
    "pagefind": "^0.12.0",
    "prettier": "^2.8.8",
    "prism-theme-night-owl": "^1.4.0"
  }
}
